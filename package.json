{
  "name": "acts-sample-front",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "npm run clean && concurrently \"npm run copy:asset\" \"npm run lint\" \"npm run build:scss\" && npm run build:ts",
    "build:ts": "webpack --progress --config ./config/webpack.config.js --hide-modules",
    "build:scss": "node config/sass.compile.js",
    "clean": "rimraf dist aot_compile_check",
    "copy:aot-check": "cpx \"src/app/**/*\" aot_compile_check/src/app/",
    "copy:asset": "cpx \"src/assets/{fonts,icon,imgs}/*\" dist/output/assets/",
    "lint": "concurrently \"npm run -s lint:html\"  \"npm run lint:scss\" \"npm run lint:ts\"",
    "lint:html": "htmlhint --config .htmlhintrc src/app/**/*.html || true",
    "lint:scss": "sass-lint -c ./.sass-lint.yml -v",
    "lint:ts": "tslint src/app/**/*.ts --force -t stylish",
    "ngc:aot-check": "ngc -p tsconfig-aot-check.json && tsc -p tsconfig-aot-check.json --noEmit || true",
    "serve": "npm run clean && concurrently \"npm run copy:asset\" \"npm run build:scss\" && npm run watch:webpack",
    "report:sonar": "sreporter",
    "verify": "npm run lint && npm run verify:aot",
    "verify:aot": "rimraf aot_compile_check && npm run copy:aot-check && npm run ngc:aot-check && rimraf aot_compile_check",
    "watch:webpack": "node ./config/watch.js"
  },
  "dependencies": {
    "@angular/animations": "5.0.1",
    "@angular/common": "5.0.1",
    "@angular/compiler": "5.0.1",
    "@angular/core": "5.0.1",
    "@angular/forms": "5.0.1",
    "@angular/http": "5.0.1",
    "@angular/platform-browser": "5.0.1",
    "@angular/platform-browser-dynamic": "5.0.1",
    "@angular/router": "5.0.1",
    "bootstrap-sass": "3.3.7",
    "core-js": "2.5.1",
    "font-awesome": "4.7.0",
    "moment": "^2.24.0",
    "ngx-bootstrap": "2.0.2",
    "ngx-mydatepicker": "2.4.9",
    "reflect-metadata": "0.1.10",
    "rxjs": "5.5.2",
    "zone.js": "0.8.18"
  },
  "devDependencies": {
    "@angular/cli": "^9.0.7",
    "@angular/compiler-cli": "5.0.1",
    "@ngtools/webpack": "1.8.2",
    "@types/requirejs": "2.1.31",
    "autoprefixer": "7.1.6",
    "browser-sync": "^2.26.7",
    "cash-true": "0.0.2",
    "clean-css": "^4.2.3",
    "codelyzer": "4.0.1",
    "concurrently": "3.5.0",
    "cpx": "1.5.0",
    "enhanced-resolve": "3.4.1",
    "extendify": "1.0.0",
    "handlebars": "^4.7.7",
    "handlebars-loader": "1.6.0",
    "html-webpack-plugin": "2.30.1",
    "htmlhint": "^0.11.0",
    "js-yaml": "^3.13.1",
    "json-loader": "0.5.7",
    "mkdirp": "0.5.1",
    "node-sass": "^4.13.1",
    "postcss": "6.0.14",
    "q": "1.5.1",
    "raw-loader": "0.5.1",
    "rimraf": "2.6.2",
    "sass-lint": "1.12.1",
    "script-loader": "0.7.2",
    "sonar-web-frontend-reporters": "^3.4.0",
    "string-replace-webpack-plugin": "0.1.3",
    "tslint": "5.8.0",
    "typescript": "2.4.2",
    "webpack": "3.8.1",
    "webpack-bundle-analyzer": "2.10.0",
    "yaml-loader": "0.5.0"
  }
}
